{
  "extends": "@oclif/tslint",
  "rules": {
    "object-curly-spacing": "always",
    "semicolon": ["error", "always", { "omitLastInOneLineBlock": true }],
    "no-console": false,
    "ter-indent": [true, 2, {
      "SwitchCase": 1
    }]
    "no-duplicate-imports": [true, {"allow-namespace-imports": true}]
  }
}
